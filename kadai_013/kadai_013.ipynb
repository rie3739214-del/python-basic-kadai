      "source": [
        "# 消費税込み金額を計算する関数\n",
        "def calc_tax_price(price, tax_rate):\n",
        "    # 消費税を計算\n",
        "    tax = price * tax_rate / 100\n",
        "    # 消費税込み金額を返す\n",
        "    return price + tax\n",
        "\n",
        "\n",
        "# 関数を呼び出す\n",
        "result = calc_tax_price(110, 10)\n",
        "\n",
        "# 結果を表示\n",
        "print(f\"消費税込み金額は{int(result)}円です\")\n"
      ]
    }
  ]
